void once() {
  while (true) {
    print("once");
    return;
  }
}
void twice() {
  int i = 1;
  while (true) {
    print("twice");
    if (i == 2) {
      return;
    }
    i++;
  }
}
void main () {
  once();
  twice();
  for (i from 1 to 10) {
    print("three times");
    if (i == 3) {
      return;
    }
  }
}